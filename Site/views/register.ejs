    <%- include('partials/head') %> 
    <%- include('partials/header2') %>

    <section class="formario">
        <section class="form">
            <form action="/users/register" method="POST" enctype="multipart/form-data">
                <h1>Regístrate</h1>
                <div class="grupo-form" >
                    <label for="">Nombre:</label>
                    <input type="text" id="barra" name="name" placeholder="Ingresá tu nombre completo" required>
                </div>
                <div class="grupo-form" >
                    <label for="">Apellido:</label>
                    <input type="text" id="barra" name="last_name" placeholder="Ingresá tu nombre completo" required>
                </div>

                <div class="grupo-form" >
                    <label for="">Email:</label>
                    <input type="email" id="barra" name="email" placeholder="Ingresá tu email" required>
                </div>

                <div class="grupo-form" >
                    <label for="">Fecha de nacimiento:</label>
                    <input type="date" id="barra" name="date" placeholder="Fecha ne nacimiento" required>
                </div>

                <div class="grupo-form">
                    <label for="">Agregar imagen</label>
                    <input type="file" name="photo">
                </div>

                <div class="grupo-form">
                    <label for="">Teléfono:</label>
                    <input type="tel" id="barra" name="phone" placeholder="Ingresá tu teléfono" required>
                </div>

                <div class="grupo-form" >
                    <label for="">Contraseña:</label>
                    <input type="password" id="barra" name="password" placeholder="Ingresá tu contraseña" required>
                </div>


                <div class="terminos-condiciones">
                    <input type="checkbox" name="terminos y condiciones" id="">
                    <label for="">Acepto terminos y condiciones</label>
                </div>

                <div class="button-algo" id="boton">
                    <button type="submit"> Registrarse</button>
                </div>
                <article class="form2" id="registrarse">
                    <div class="algo2">
                        <a href="/users/login">¿Ya tienes cuenta? <b>Inicia sesión.</b></a>
                    </div>
                </article>
            </form>
        </section>
    </section>

