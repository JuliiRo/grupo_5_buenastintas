<body>
    <%- include('partials/header2') %>
    
    
    <section class="formario">
        <section class="form">
            <form action="">
                <h1>Regístrate </h1>
                <div class="grupo-form" >
                    <label for="">Nombre completo:</label>
                    <input type="text" id="barra" placeholder="Ingresá tu nombre completo" required>
                </div>

                <div class="grupo-form" >
                    <label for="">Email:</label>
                    <input type="email" id="barra" placeholder="Ingresá tu email" required>
                </div>

                <div class="grupo-form" >
                    <label for="">Fecha de nacimiento:</label>
                    <input type="date" id="barra" placeholder="Fecha ne nacimiento" required>
                </div>


                <div class="grupo-form" >
                    <label for="">Teléfono:</label>
                    <input type="tel" id="barra" placeholder="Ingresá tu teléfono" required>
                </div>

                <div class="grupo-form" >
                    <label for="">Contraseña:</label>
                    <input type="password" id="barra" placeholder="Ingresá tu contraseña" required>
                </div>


                <div class="grupo-form2">
                    <input type="checkbox" name="terminos y condiciones" id="">
                    <label for="">Acepto terminos y condiciones</label>
                </div>

                <div class="button">
                    <button type="submit"> Registrarse</button>
                </div>
                <article class="form2" id="iniciarsesion">
                    <div class="algo2">
                        <a href="/users">¿Ya tienes cuenta? <b>Iniciar sesión.</b></a>
                    </div>
                </article>
            </form>
        </section>
    </section>

